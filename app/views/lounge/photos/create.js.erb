<% if @photo.new_record? %>
  alert("Failed to upload photo: <%= j @photo.errors.full_messages.join(', ').html_safe %>");
<% else %>
  $("#photos ul.thumbnails").append("<%= j render(@photo) %>");
  $('.profile-progress.bar').css('width', "<%= j current_user.user_profile.percent_complete.to_s %>%")
  $('.upload .progress .bar').parent().parent().fadeOut()
<% end %>